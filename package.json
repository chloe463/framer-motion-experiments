{
  "name": "framer-motion-experiments",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.13",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "apollo": "^2.32.5",
    "framer-motion": "^4.0.2",
    "graphql": "^15.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-virtualized": "^9.22.3",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"yarn codegen:types:watch\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "codegen:download-shcema": "apollo client:download-schema --endpoint http://localhost:4000/graphql schema.json",
    "codegen:types": "apollo client:codegen --target=typescript --includes='src/**/*.{ts,tsx}' --localSchemaFile=./schema.json --globalTypesFile=src/__generated__/globalTypes.d.ts --tsFileExtension='d.ts'",
    "codegen:types:watch": "yarn codegen:types --watch",
    "clean:types": "./scripts/clean-types.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-virtualized": "^9.21.11",
    "@types/styled-components": "^5.1.9",
    "concurrently": "^6.0.0"
  }
}
